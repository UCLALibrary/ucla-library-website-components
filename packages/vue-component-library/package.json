    "vue-tsc": "catalog:"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^8.2.0",
    "@vueuse/components": "^11.1.0",
    "@vueuse/core": "^11.1.0",
    "add-to-calendar-button": "^2.6.18",
    "date-fns": "^2.30.0",
