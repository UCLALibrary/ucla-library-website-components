    "@storybook/vue3-vite": "^7.1.1",
    "@tsconfig/node18": "^18.2.4",
    "@types/lodash": "^4.17.15",
    "@types/node": "^18.19.55",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.15.0",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-vue": "^9.17.0",
    "http-server": "^14.1.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.79.5",
    "storybook": "^7.4.1",
    "typescript": "catalog:",
    "ucla-library-design-tokens": "^5.30.0",
    "video.js": "^8.5.2",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.0",
