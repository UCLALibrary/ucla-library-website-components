    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.45.0",
    "sass-loader": "^10.1.1",
    "ucla-library-design-tokens": "^3.0.0",
    "vue": "^2.6.14",
    "vue-cli-plugin-storybook": "~2.1.0",
    "vue-template-compiler": "^2.6.14"
